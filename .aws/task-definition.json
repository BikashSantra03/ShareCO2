{
    "family": "share-co2",
    "containerDefinitions": [
        {
            "cpu": 2048,
            "environment": [],
            "environmentFiles": [
                {
                    "type": "s3",
                    "value": "arn:aws:s3:::shareco2/prod/env/prod.env"
                }
            ],
            "essential": true,
            "image": "218903530701.dkr.ecr.ap-south-1.amazonaws.com/share-co2:latest",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/share-co2",
                    "awslogs-create-group": "true",
                    "awslogs-region": "ap-south-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "memory": 4096,
            "memoryReservation": 2048,
            "mountPoints": [],
            "name": "share-co2",
            "portMappings": [
                {
                    "appProtocol": "http",
                    "containerPort": 80,
                    "hostPort": 80,
                    "name": "80",
                    "protocol": "tcp"
                },
                {
                    "appProtocol": "http",
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "name": "3000",
                    "protocol": "tcp"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        }
    ],
    "executionRoleArn": "arn:aws:iam::218903530701:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "2048",
    "memory": "4096",
    "runtimePlatform": {
        "cpuArchitecture": "ARM64",
        "operatingSystemFamily": "LINUX"
    },
    "enableFaultInjection": true
}