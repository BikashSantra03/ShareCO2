{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "crons": [
    {
      "path": "/api/send-reminders?type=morning",
      "schedule": "30 1,2 * * *"
    },
    {
      "path": "/api/send-reminders?type=evening",
      "schedule": "0 10,11,12 * * *"
    },
    {
      "path": "/api/send-reminders?type=evening",
      "schedule": "* * * * *"
    }
  ]
}